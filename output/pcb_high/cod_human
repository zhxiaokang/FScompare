awk -F'\t' 'NR==FNR{a[$1]=$2} NR>FNR && a[$1] != "" {print a[$1]}' 1125_cod_ptns_conv_to_human_ptns.txt protein_geode_uniq.txt > protein_geode_human.txt
awk -F'\t' 'NR==FNR{a[$1]=$2} NR>FNR && a[$1] != "" {print a[$1]}' 1125_cod_ptns_conv_to_human_ptns.txt protein_mrmr_uniq.txt > protein_mrmr_human.txt
awk -F'\t' 'NR==FNR{a[$1]=$2} NR>FNR && a[$1] != "" {print a[$1]}' 1125_cod_ptns_conv_to_human_ptns.txt protein_sam_uniq.txt > protein_sam_human.txt